.fields
  %table.grid-selection{:cellpadding => 0, :cellspacing => 0}
    %col{:width => 200}
    %tr
      %td.first
        %span
          -if !f.object.new_record?
            = @usuario.nombres 
        = f.hidden_field :usuario_id, :class => "proyecto_usuarios_hidden"
      %td
        Administrar
        = f.check_box :administrador
      %td
        = remove_child_link "Eliminar", f 
 
