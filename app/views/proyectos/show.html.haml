= render(:partial => "shared/adminmenu")
.component-navigator-tabs
  %ul
    %li.tab-select.first
      %a#tab-form{:href => "javascript:void(0)"} Proyecto
    %li.clearfix
.component-search
  .component-search-forms.forms-two-cols
    %p 
      %b Nombre:
      = h @proyecto.descripcion
    %p 
      %b Código:
      = h @proyecto.codigo
    %p 
      %b Estimación:
      = h @proyecto.estimacion
    %p 
      %b Archivado:
      = h @proyecto.archivado
    %p 
      %b Privado:
      = h @proyecto.privado
    %p
      %b Cliente:
      = h @proyecto.cliente.descripcion
    = link_to 'Editar', edit_proyecto_path(@proyecto)
    |
    \#{link_to 'Listado', proyectos_path}
