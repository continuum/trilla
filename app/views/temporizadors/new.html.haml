%h1 New temporizador
- form_for(@temporizador) do |f|
  = f.error_messages
  %p
    = f.label :descripcion
    %br/
    = f.text_field :descripcion
  %p
    = f.label :start
    %br/
    = f.datetime_select :start
  %p
    = f.label :stop
    %br/
    = f.datetime_select :stop
  %p
    = f.label :proyecto
    %br/
    = f.text_field :proyecto
  %p
    = f.label :tarea
    %br/
    = f.text_field :tarea
  %p
    = f.label :usuario
    %br/
    = f.text_field :usuario
  %p
    = f.submit 'Create'
= link_to 'Back', temporizadors_path
