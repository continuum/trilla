<h1>New proyecto_usuario</h1>

<% form_for(@proyecto_usuario) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :proyecto %><br />
    <%= f.text_field :proyecto %>
  </p>
  <p>
    <%= f.label :usuario %><br />
    <%= f.text_field :usuario %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', proyecto_usuarios_path %>