<h1>Listing proyecto_usuarios</h1>

<table>
  <tr>
    <th>Proyecto</th>
    <th>Usuario</th>
  </tr>

<% @proyecto_usuarios.each do |proyecto_usuario| %>
  <tr>
    <td><%=h proyecto_usuario.proyecto %></td>
    <td><%=h proyecto_usuario.usuario %></td>
    <td><%= link_to 'Show', proyecto_usuario %></td>
    <td><%= link_to 'Edit', edit_proyecto_usuario_path(proyecto_usuario) %></td>
    <td><%= link_to 'Destroy', proyecto_usuario, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New proyecto_usuario', new_proyecto_usuario_path %>