= render(:partial => "shared/adminmenu")
.component-navigator-tabs
  %ul
    %li.tab-select 
      %a#tab-form{:href => "javascript:void(0)"} Editar Usuario
    %li.clearfix
.component-search
  - form_for(@usuario) do |f|
    = f.error_messages
    .component-search-forms.forms-two-cols    
      = render :partial => 'fields', :locals => { :f => f}   
      .clearfix
    .component-search-actions
      = f.submit 'Actualizar'
      |
      \#{link_to 'Cancelar', usuarios_path}
